       "\t\t\t\t}\n",
       "\t\t} else if (charC == 47) {\n",
       "\t\t\t\tif (el.value.indexOf('/') === -1) {\n",
       "            if ((el.value != \"\") && (el.value != \".\")) {\n",
       "\t\t\t\t\t      return true;\n",
       "            } else {\n",
       "\t\t\t\t\t  return false;\n",
       "\t\t\t\t    }\n",
       "        } else {\n",
       "            return false;\n",
       "        }\n",
       "\t\t} else if (charC == 45) {\n",
       "        if (el.value == \"\" ) {\n",
       "            return true;\n",
       "        } else {\n",
       "            return false;\n",
       "        }\n",
       "    } else {\n",
       "\t\t\t\tif (charC > 31 && (charC < 48 || charC > 57))\n",
